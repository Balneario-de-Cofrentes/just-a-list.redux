{
  "name": "@mutantlove/redux-list",
  "version": "0.7.0",
  "description": "Treat Redux state slices as lists with a standard structure and behaviour",
  "homepage": "https://github.com/mutantlove/redux-list",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutantlove/redux-list.git"
  },
  "bugs": {
    "url": "https://github.com/mutantlove/redux-list/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "main": "dist/index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTILS": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update --save-exact",
    "----BUILD": "",
    "build": "rm -rf ./dist && babel src -d dist",
    "prepare": "npm audit fix && npm test && npm run lint && npm run build",
    "prepublishOnly": "npm run coverage",
    "----LINT": "",
    "lint:js": "eslint ./src",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "test": "tape -r @babel/register src/*.test.js src/**/*.test.js | tap-nirvana",
    "tdd": "nodemon --watch src --exec \"npm run test\"",
    "coverage:report": "NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.7.4",
    "@mutantlove/m": "1.4.0",
    "core-js": "3.4.3",
    "regenerator-runtime": "0.13.3"
  },
  "devDependencies": {
    "@asd14/eslint-config": "5.2.0",
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-pipeline-operator": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/register": "7.7.4",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-no-inferred-method-name": "2.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "13.0.0",
    "markdownlint-cli": "0.19.0",
    "nodemon": "2.0.1",
    "npm-check": "5.9.0",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "react-redux": "7.1.3",
    "redux": "4.0.4",
    "semantic-release": "15.13.31",
    "tap-nirvana": "1.1.0",
    "tape": "4.11.0"
  }
}
