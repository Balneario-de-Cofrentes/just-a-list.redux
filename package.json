  "scripts": {
    "----UTILS------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "build": "rm -rf ./dist && babel src -d dist",
    "prepare": "npm audit fix && npm test && npm run lint && npm run build",
    "prepublishOnly": "npm run coverage",
    "----TESTS------------------------------------": "",
    "lint": "eslint ./src",
    "test": "tape -r @babel/register src/*.test.js src/**/*.test.js",
    "tdd": "nodemon --watch src --exec \"npm run test | tap-diff\"",
    "coverage:report": "NODE_ENV=test nyc npm test && nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "peerDependencies": {
    "redux": "^4.0.1"
  },
  "dependencies": {
    "@asd14/m": "0.15.2",
    "@babel/runtime": "7.1.5",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@asd14/eslint-config": "3.0.2",
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.2",
    "nyc": "13.1.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.3.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "6.0.1",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.6",
    "npm-check": "5.9.0",
    "prettier": "1.15.2",
    "tap-diff": "0.1.1",
    "tape": "4.9.1"
  }
}
